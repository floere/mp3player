 713:main.c        **** // Enables the radio.
 714:main.c        **** //
 715:main.c        **** void enableRadio(void) {
 716:main.c        ****   radio_enable = ON;
 717:main.c        ****   
 718:main.c        ****   // graphics
 719:main.c        ****   LCDSetRowColor(2, 0, current_display->back_color, current_display->orientation);
 720:main.c        ****   LCDPrintString("On", 0, current_display->text_color, 2,0,current_display->orientation);
 721:main.c        ****   
 722:main.c        ****   // send data
 723:main.c        ****   IOCLR1 |= FM_CS;        //Select the FM transmitter
 724:main.c        ****   delay_ms(100);
 725:main.c        ****   ns73Config();           //Configure the FM Trans. I/O
 726:main.c        ****   ns73Send(R0, PE | AG);  //Power up the radio
 727:main.c        ****   IOSET1 |= FM_CS;        //Unselect the FM transmitter
 728:main.c        **** }
