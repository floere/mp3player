 697:main.c        **** 
 698:main.c        **** // initializes the FM transmitter to a given frequency.
 699:main.c        **** // Frequency is given in tenths of a MHz. So 973 means 97.3 MHz.
 700:main.c        **** //
 701:main.c        **** void initializeRadio(int frequency) {
 702:main.c        ****   IOCLR1 |= FM_CS;      //Select SPI for FM Transmitter
 703:main.c        ****   delay_ms(900);
 704:main.c        ****   
 705:main.c        ****   ns73Config();         //Configigure the FM Trans. I/O
 706:main.c        ****   ns73Init();           //Setup the Default Register Values
 707:main.c        ****   ns73SetChannel(frequency);  //Transmit to 97.3 FM
 708:main.c        ****   
 709:main.c        ****   IOSET1 |= FM_CS;      //Remove FM Transmitter from SPI bus
 710:main.c        ****   delay_ms(100);
 711:main.c        **** }
